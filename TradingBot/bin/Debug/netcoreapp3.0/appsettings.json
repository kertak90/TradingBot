{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [ { "Name": "Console" } ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "With",
        "Args": { "policy": "Sample.CustomPolicy, Sample" }
      }
    ],
    "Properties": {
      "Application": "Sample"
    }
  },
  "AllowedHosts": "*",
  "TinkoffSettings": {
    "TokenPath": {
      "Windows": "C:\\Settings\\TinkoffBrokerToken.txt",
      "Linux": "/usr/local/TinkoffBrokerToken.txt"
    },
    "TinkoffOpenApiBaseAdress": "https://api-invest.tinkoff.ru/openapi"
  },
  "TelegramBotSettings": {
    "TokenPath": {
      "Windows": "C:\\Settings\\TelegramBotToken.txt",
      "Linux": "/usr/local/TelegramBotToken.txt"
    },
    "ChatId": "-1001508816220"
  }
}